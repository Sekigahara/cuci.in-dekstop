        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cuciin_Dekstop.Login"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="username_field" HorizontalAlignment="Left" Height="auto" Margin="224,121,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="344"/>
        <TextBlock HorizontalAlignment="Left" Margin="224,100,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="228,185,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top"/>
        <Button x:Name="login_button" Content="Login" HorizontalAlignment="Left" Margin="323,283,0,0" VerticalAlignment="Top" Width="144" Height="34"/>
        <TextBox x:Name="password_field" HorizontalAlignment="Left" Height="auto" Margin="224,232,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="344"/>
        <TextBlock x:Name="status_field" HorizontalAlignment="Left" Margin="224,356,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="344"/>
    </Grid>
</basic:MyPage>
